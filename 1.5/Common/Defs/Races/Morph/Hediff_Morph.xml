<?xml version="1.0" encoding="utf-8"?>
<Defs>

		
	<!-- Human -->		
		
		<!-- Morph Human -->
			
			<HediffDef ParentName="IsekaiHediffBase">
				<defName>Isekai_Morph_Human</defName>
				<label>Human morphing</label>
				<description>Morphing into a human...</description>
				<isBad>false</isBad>
				<everCurableByItem>false</everCurableByItem>
				<initialSeverity>1</initialSeverity>
				<comps>
					<li Class="EBSGFramework.HediffCompProperties_AlterXenotype">
						<xenotypes>
							<Isekai_Morph_Human>1</Isekai_Morph_Human>
						</xenotypes>
						<severities>1~999</severities>
						<setXenotype>False</setXenotype>
					</li>
				</comps>
				<stages>
				</stages>
			</HediffDef>
		

		<!-- XenoChange Human -->				<!-- only for visuals -->
			
			<HediffDef ParentName="IsekaiHediffBase">
				<defName>Isekai_XenoChange_Human</defName>
				<label>Human morphing</label>
				<description>Morphing into a human...</description>
				<isBad>false</isBad>
				<everCurableByItem>false</everCurableByItem>
				<initialSeverity>1</initialSeverity>
				<comps>
					<li Class="EBSGFramework.HediffCompProperties_AlterXenotype">
						<xenotypes>
							<Isekai_XenoChange_Human>1</Isekai_XenoChange_Human>
						</xenotypes>
						<severities>1~999</severities>
						<setXenotype>True</setXenotype>		<!-- Changed -->
					</li>
				</comps>
				<stages>
				</stages>
			</HediffDef>



		
		
		
		
		
		
</Defs>
