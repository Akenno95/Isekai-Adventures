<?xml version="1.0" encoding="utf-8"?>
<Defs>
	
		<!-- Resistances -->
		
		<GeneDef MayRequire = "Ludeon.RimWorld.Biotech">
			<defName>Isekai_Resistances_Gene</defName>
			<label>Resistances</label>
			<description>This gene activates the ability to gain resistances for damage types.</description>
			
			<!-- <iconPath>UI/Icons/Genes/...</iconPath> -->
			<displayCategory>Isekai_Genes</displayCategory>
			<displayOrderInCategory>100</displayOrderInCategory>
			
			<modExtensions>
				<li Class="ItsSorceryFramework.ModExtension_SchemaAddition">
					<schema>Isekai_Resistances</schema>
				</li>
			</modExtensions>
			<!--biostatMet>-1</biostatMet-->
			<!-- <symbolPack>
				<prefixSymbols>
					<li><symbol>...</symbol></li>
				</prefixSymbols>
			</symbolPack> -->
		</GeneDef>



		<!-- Body -->
		
		<GeneDef MayRequire = "Ludeon.RimWorld.Biotech">
			<defName>Isekai_Body_Gene</defName>
			<label>Rank</label>
			<description>This gene activates the ability to gain body enhancements for your pawn, also improving base pawn stats.</description>
			
			<!-- <iconPath>UI/Icons/Genes/...</iconPath> -->
			<displayCategory>Isekai_Genes</displayCategory>
			<displayOrderInCategory>101</displayOrderInCategory>
			
			<modExtensions>
				<li Class="ItsSorceryFramework.ModExtension_SchemaAddition">
					<schema>Isekai_Body</schema>
				</li>
			</modExtensions>
			<!--biostatMet>-1</biostatMet-->
			<!-- <symbolPack>
				<prefixSymbols>
					<li><symbol>...</symbol></li>
				</prefixSymbols>
			</symbolPack> -->
		</GeneDef>
		
		
		<!-- Magic -->
		
		<GeneDef MayRequire = "Ludeon.RimWorld.Biotech">
			<defName>Isekai_Magic_Gene</defName>
			<label>Magic</label>
			<description>This gene activates the ability to gain Magic abilities for your pawn.</description>
			
			<!-- <iconPath>UI/Icons/Genes/...</iconPath> -->
			<displayCategory>Isekai_Genes</displayCategory>
			<displayOrderInCategory>102</displayOrderInCategory>
			
			<modExtensions>
				<li Class="ItsSorceryFramework.ModExtension_SchemaAddition">
					<schema>Isekai_Magic</schema>
				</li>
			</modExtensions>
			<!--biostatMet>-1</biostatMet-->
			<!-- <symbolPack>
				<prefixSymbols>
					<li><symbol>...</symbol></li>
				</prefixSymbols>
			</symbolPack> -->
		</GeneDef>



		<!-- Adventurer -->
		
		<GeneDef MayRequire = "Ludeon.RimWorld.Biotech">
			<defName>Isekai_Adventurer_Gene</defName>
			<label>Adventurer</label>
			<description>This gene is in every adventurer out there. It gives you multiple genes at once, for example the ability to cast magic or to strengthen your body.</description>
			<geneClass>EBSGFramework.HediffAdder</geneClass>
			
			<!-- <iconPath>UI/Icons/Genes/...</iconPath> -->
			<displayCategory>Isekai_Genes</displayCategory>
			<displayOrderInCategory>99</displayOrderInCategory>
			
			<modExtensions>
				<li Class="EBSGFramework.EBSGExtension">
					<hediffsToApply>
						<li>
							<hediff>Isekai_Adventurer</hediff>
							<!--onlyIfNew>true</onlyIfNew>
							<severity>1</severity-->
						</li>
					</hediffsToApply>
				</li>
			</modExtensions>
			<!--biostatMet>-1</biostatMet-->
			<!--symbolPack>
				<prefixSymbols>
					<li><symbol>...</symbol></li>
				</prefixSymbols>
			</symbolPack>-->
		</GeneDef>





</Defs>
