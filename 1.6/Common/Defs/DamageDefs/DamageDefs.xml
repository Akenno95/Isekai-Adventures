<?xml version="1.0" encoding="utf-8"?>
<Defs>


		<DamageDef>
			<defName>Isekai_SlashDamage</defName>
			<label>Wind cut</label>
			<workerClass>DamageWorker_AddInjury</workerClass>
			<externalViolence>true</externalViolence>
			<deathMessage>{0} has been cut to death threw sharp magic.</deathMessage>
			<hediff>Isekai_Cut_Small</hediff>
			<!-- <hediffSolid>Crush</hediffSolid> -->
			<harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
			<impactSoundType>Slice</impactSoundType>
			<armorCategory>Magic</armorCategory>
			<overkillPctToDestroyPart>0.0~0.1</overkillPctToDestroyPart>
			<isRanged>true</isRanged>
			<makesAnimalsFlee>true</makesAnimalsFlee>
			<isExplosive>false</isExplosive>
		</DamageDef>

		<DamageDef ParentName="Flame">
			<defName>Isekai_Damage_Holy</defName>
			<label>Holy burn</label>
			<workerClass>DamageWorker_Flame</workerClass>
			<deathMessage>{0} has been burned by holy magic.</deathMessage>
			<hediff>Isekai_Hediff_Holy_Burn_Low</hediff>
			<armorCategory>Isekai_Light</armorCategory>
			<overkillPctToDestroyPart>0.0~0.1</overkillPctToDestroyPart>
		</DamageDef>











</Defs>
